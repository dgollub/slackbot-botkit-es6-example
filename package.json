    "botkit": "latest",
    "eslint": "^1.10.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bluebird": "^3.1.1",
    "lodash": "^3.10.1",
    "nodegit": "^0.6.3",
